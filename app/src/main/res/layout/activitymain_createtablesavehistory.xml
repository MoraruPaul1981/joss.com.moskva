<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:scrollbars="horizontal|vertical"
    android:verticalScrollbarPosition="defaultPosition"
    android:visibility="visible"
    tools:context=".Code_ForTABEL.MainActivity_New_Tabely"
    tools:visibility="visible">


    <TextView
        android:id="@+id/textView4"
        style="@style/Widget.MaterialComponents.MaterialCalendar.DayTextView"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="40dp"
        android:bufferType="normal"
        android:capitalize="characters"
        android:fontFamily="sans-serif-light"
        android:foregroundGravity="center"
        android:gravity="center"
        android:lineSpacingExtra="30sp"
        android:text="Создание  Табеля "
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="#0E0E0E"
        android:textSize="16sp"
        android:textStyle="bold"
        android:theme="@style/Theme.AppCompat.DayNight"
        android:typeface="monospace"
        app:layout_constraintBottom_toTopOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/КнопкаНазадСтрелкаСозданиеНовгоТабеля"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/КнопкаНазадСтрелкаСозданиеНовгоТабеля"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_margin="1dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:backgroundTintMode="screen"
        android:stateListAnimator="@drawable/icon_dsu1_exit3_foreground"
        android:textAlignment="center"
        android:visibility="visible"
        app:icon="@drawable/icon_dsu1_exit3_foreground"
        app:iconGravity="textEnd"
        app:iconSize="40dp"
        app:iconTint="#00ACC1"
        app:layout_constraintBottom_toBottomOf="@id/view3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.263"
        app:rippleColor="#F50057" />

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#00ACC1"
        android:foregroundGravity="center"
        android:visibility="visible"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:visibility="visible" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/КнопкаСозданиеНовогоТабеля"
        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton.Icon"
        android:layout_width="164dp"
        android:layout_height="40dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="50dp"
        android:width="100dp"
        android:background="#0E0E0E"
        android:backgroundTint="#00ACC1"
        android:focusable="auto"
        android:fontFamily="sans-serif-medium"
        android:foregroundGravity="center"
        android:gravity="center|center_vertical"
        android:text="Создать"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#0E0E0E"
        android:textColorHint="#0E0E0E"
        android:textSize="11dp"
        android:textStyle="bold"
        android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
        android:visibility="visible"
        app:backgroundTint="#00E5FF"
        app:cornerRadius="20dp"
        app:iconTint="#8A3C3C"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ЗначениеДатаСоздаваемогоТабеля"
        tools:visibility="visible" />


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/textviewnamecfo"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/style_for_assitiionmatrial_create_name"
        android:gravity="center_vertical|center_horizontal"
        android:hint="Цфо"
        android:textColor="#00E5FF"
        android:textColorHint="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        android:translationZ="5dp"
        android:visibility="visible"
        app:cardCornerRadius="1dp"
        app:cardElevation="1dp"
        app:layout_constraintBottom_toTopOf="@+id/materialspiritnew_tabel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/materialspiritnew_tabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="40dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="5dp"
        android:layout_marginStart="5dp"
        android:background="@drawable/style_forpolymaterialb_cycle_createspinner"
        android:gravity="center"
        android:textAlignment="gravity"
        android:textColor="#000000"
        android:textSize="15sp"
        android:textStyle="normal"
        android:visibility="visible"
        android:padding="5dp"
        android:hint="Выберете цфо"
        android:textColorHint="#80807C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/textviewdate"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/style_for_assitiionmatrial_create_name"
        android:gravity="center_vertical|center_horizontal"
        android:hint="Дата"
        android:textColor="#00E5FF"
        android:textColorHint="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        android:translationZ="5dp"
        android:visibility="visible"
        app:cardCornerRadius="1dp"
        app:cardElevation="1dp"
        app:layout_constraintBottom_toTopOf="@+id/ЗначениеДатаСоздаваемогоТабеля"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/ЗначениеДатаСоздаваемогоТабеля"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="40dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="5dp"
        android:layout_marginStart="5dp"
        android:background="@drawable/style_forpolymaterialb_cycle_createspinner"
        android:gravity="center"
        android:textAlignment="gravity"
        android:textColor="#000000"
        android:textSize="15sp"
        android:textStyle="bold"
        android:visibility="visible"
        android:padding="5dp"
        android:hint="Выберете дату"
        android:textColorHint="#80807C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/materialspiritnew_tabel" />

    <ProgressBar
        android:id="@+id/ProgressBar"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_centerHorizontal="true"
        android:background="#FFFFFF"
        android:indeterminate="true"
        android:scaleX="1"
        android:scaleY="3"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>
