<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/TableLayoutSingleTabel"
    android:shrinkColumns="*"
    android:stretchColumns="*"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:gravity="center_horizontal|center_vertical"
    android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
    tools:context=".Code_ForTABEL.MainActivity_Tabel_Single_People">


    <TableRow android:id="@+id/TableRowsContenerSingleTabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        android:gravity="center_vertical|center_horizontal">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/RowКонтейнерКудаЗагружаетьсяФИО"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="3dp"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:layout_span="4"
            android:background="#FFFFFF"
            android:foregroundGravity="center"
            android:gravity="center_vertical|center_horizontal"
            android:hint="ФИО+Профессия"
            android:minWidth="100dp"
            android:minHeight="35dp"
            android:textAlignment="gravity"
            android:textColor="#000000"
            android:textSize="13sp"
            android:textStyle="bold"
            android:visibility="visible"
            app:cardCornerRadius="15dp"
            app:cardElevation="5dp"
            android:padding="2dp"
            app:strokeColor="#F5FFFA" />
    </TableRow>

    <TableRow android:id="@+id/TableOneRowLine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        android:gravity="center_vertical|center_horizontal">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/line1singletabel"
            android:layout_span="5"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:minWidth="100dp"
            android:layout_marginStart="2dp"
            android:layout_marginEnd="2dp"
            android:clickable="true"
            android:focusable="auto"
            android:fontFamily="sans-serif-medium"
            android:gravity="center_horizontal|center_vertical"
            android:textStyle="bold"
            android:typeface="normal"
            android:visibility="visible"
            app:cornerRadius='150dp'
            app:cardElevation="5dp"
            tools:visibility="visible"
            android:translationZ="100dp"
            android:background="@drawable/StyleSingleTableRowLIne2"
            android:animateLayoutChanges="true" />
    </TableRow>






