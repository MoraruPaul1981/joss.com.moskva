<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Relativemain_for_chats"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Code_For_Chats_КодДля_Чата.Fragment_Contacts_КонтактыЧата">

    <!-- TODO: Update blank fragment layout -->


    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:id="@+id/textViewФрагментКонтактыДляЧата"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="Контакты"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#00ACC1"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/viewФрагментСообщенияНазваниеЧАты2"
