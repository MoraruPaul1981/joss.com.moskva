<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/TableLayoutAdmissionLayoutInflater_row"
    android:shrinkColumns="*"
    android:stretchColumns="*"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal|center_vertical"
    android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
    android:background="#F5FFFA"
    tools:context=".Code_For_AdmissionMaterials.ActivityFragment.MainActivity_AdmissionMaterials">




    <TableRow android:id="@+id/tablerowCFO_detals"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        android:gravity="center_vertical|center_horizontal">

        <TextView
            android:id="@+id/textviewname_detalis_cfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="3dp"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:layout_span="3"
            android:background="@drawable/style_forpolymaterialb"
            android:foregroundGravity="center"
            android:gravity="center_vertical|center_horizontal"
            android:hint="Детализация"
            android:minWidth="100dp"
            android:minHeight="25dp"
            android:textAlignment="gravity"
            android:textColor="#000000"
            android:textSize="13sp"
            android:textStyle="bold"
            android:visibility="visible"
            app:cardCornerRadius="15dp"
            app:cardElevation="5dp"
            android:padding="2dp"
            app:strokeColor="#F5FFFA" />
    </TableRow>

    <TableRow
        android:id="@+id/tablerowline_detaliz"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        android:gravity="center_vertical|center_horizontal">
        <Button
            android:id="@+id/linebottom22"
            android:layout_span="3"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:minWidth="100dp"
            android:backgroundTint="#7B7879"
            android:layout_marginStart="2dp"
            android:layout_marginEnd="2dp"
            android:clickable="true"
            android:focusable="auto"
            android:fontFamily="sans-serif-medium"
            android:gravity="center_horizontal|center_vertical"
            android:textStyle="bold"
            android:typeface="normal"
            android:visibility="visible"
            app:cornerRadius='150dp'
            app:cardElevation="5dp"
            tools:visibility="visible"
            android:translationZ="100dp"
            android:animateLayoutChanges="true" />
    </TableRow>






    <TableRow
        android:id="@+id/tableName_for_detalisaziy"
        android:layout_width="match_parent"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginTop="2dp"
        android:gravity="center_vertical|center_horizontal"
        android:layout_height="wrap_content"
        android:minHeight="30dp">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/textview1Name77"
            android:layout_width="70dp"
            android:layout_height="25dp"
            android:layout_column="0"
            android:layout_margin="1dp"
            android:text="Тип материала"
            android:textColor="#FFFFFF"
            android:background="@drawable/style_forpolymaterialb3"
            android:gravity="center_vertical|center_horizontal"
            android:padding="1dp"
            android:textAlignment="gravity"
            android:textSize="11sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textviewНазваниеВесовой888"
            android:layout_width="70dp"
            android:layout_height="25dp"
            android:layout_column="1"
            android:layout_margin="1dp"
            android:background="@drawable/style_forpolymaterialb3"
            android:foregroundGravity="center"
            android:gravity="center"
            android:text="Материал"
            android:textColor="#FFFFFF"

            android:padding="1dp"
            android:textAlignment="gravity"
            android:textSize="11sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textview3Количество444"
            android:layout_width="70dp"
            android:layout_height="25dp"
            android:layout_column="2"
            android:layout_margin="1dp"
            android:background="@drawable/style_forpolymaterialb3"
            android:gravity="center_vertical|center_horizontal"
            android:text="Количество"
            android:textColor="#FFFFFF"
            android:padding="1dp"
            android:textAlignment="gravity"
            android:textSize="11sp"
            android:textStyle="bold"
            android:visibility="visible" />

    </TableRow>











    <TableRow
        android:id="@+id/tableData_for_detalisaziy"
        android:layout_width="match_parent"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="2dp"
        android:gravity="center_vertical|center_horizontal"
        android:layout_height="wrap_content"
        android:minHeight="40dp">
        <TextView
            android:id="@+id/textview_data_det_type"
            android:layout_column="0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/style_forpolymaterialb_cycle"
            android:foregroundGravity="center"
            android:gravity="center_vertical|center_horizontal"
            android:hint="тип"
            android:layout_margin="1dp"
            android:minHeight="40dp"
            android:minWidth="70dp"
            android:textAlignment="gravity"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold"
            android:padding="2dp"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textview_data_det_material"
            android:layout_column="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="40dp"
            android:minWidth="70dp"
            android:layout_margin="1dp"
            android:background="@drawable/style_forpolymaterialb_cycle"
            android:gravity="center_vertical|center_horizontal"
            android:hint="материал"
            android:textAlignment="gravity"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold"
            android:padding="2dp"
            android:visibility="visible" />
        <TextView
            android:id="@+id/textview_data_det_kilichestvo"
            android:layout_column="2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="40dp"
            android:minWidth="70dp"
            android:layout_margin="1dp"
            android:gravity="center_vertical|center_horizontal"
            android:background="@drawable/style_forpolymaterialb_cycle"
            android:hint="количество"
            android:textAlignment="gravity"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold"
            android:padding="2dp"
            android:visibility="visible" />
    </TableRow>








</TableLayout>