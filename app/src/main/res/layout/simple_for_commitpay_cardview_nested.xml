    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardview_commingpay_nesteds"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="5dp"
    app:cardElevation="5dp"
    app:strokeColor="#ABB1B1"
    app:cardBackgroundColor="#F1F4F9"
    app:cardMaxElevation="30dp"
    app:strokeWidth="1dp"
    android:translationZ="100dp"
    android:animateLayoutChanges="true"
    android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
    tools:context=".PaysCommings.View.Window.MainActivity_CommitPay">




    <TableLayout
        android:shrinkColumns="*"
        android:stretchColumns="*"
        android:gravity="center_horizontal|center_vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="visible"
            android:layout_margin="2dp"
            android:background="@drawable/disayn_line_tablerow10"
            android:gravity="center_vertical|center_horizontal">

            <ProgressBar
                android:id="@+id/progressbar_commingpay_nesteds"
                style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                android:layout_above="@+id/BottomNavigationView"
                android:layout_width="match_parent"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                android:indeterminateTint="#C2C4C5"
                android:layout_height="3dp"
                android:scaleY="4"
                android:scaleX="1"
                android:layout_centerHorizontal="true"
                android:visibility="visible"
                android:indeterminate="true" />

        </TableRow>






        <!-- todo ДАННЫЕ первые строчки  #1-->
