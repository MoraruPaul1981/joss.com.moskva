                    android:hint="1"
                    android:minWidth="120dp"
                    android:minHeight="20dp"
                    android:textAlignment="gravity"
                    android:textColor="#000000"
                    android:textColorHint="#454542"
                    android:textSize="11sp"
                    android:textStyle="normal"
                    android:tooltipText="Сумма"
