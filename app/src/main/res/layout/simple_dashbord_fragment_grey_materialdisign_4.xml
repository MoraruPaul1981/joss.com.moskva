<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativelayout_settings"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"




    android:translationZ="0dp"
    app:cardElevation="0dp"
    app:cardMaxElevation="0dp"
    tools:context=".Dashboard.MainActivity_Dashboard">























    <!--       ///TODO TOOLBAR  toolbarcamera_settigs -->
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarcamera_dashbord"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_above="@+id/КнопкаоСистеме"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginBottom="5dp"
        android:background="#FFFFFF"
        android:elevation="0dp"
        app:showAsAction="always"

        app:theme="@style/AppTheme">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">



            <androidx.appcompat.widget.AppCompatImageButton
                android:id="@+id/imageButton_back_in_settings"
                android:layout_alignLeft="@+id/TextViewLogo"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:alpha="0.5"
                android:layout_marginTop="10dp"
                android:src="@drawable/icon_new_settings_arrow_back"
                app:rippleColor="@color/colorPrimaryDark"
                android:background="?attr/selectableItemBackgroundBorderless"
                android:foreground="?attr/selectableItemBackground"
                tools:ignore="SpeakableTextPresentCheck" />


            <TextView
                android:id="@+id/TextViewLogo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:fontFamily="@font/alegreya_sans_sc_thin"
                android:gravity="center"
                android:hint="Настройки"
                android:text="Настройки"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="36sp"
                android:textStyle="bold"
                android:visibility="visible" />


        </RelativeLayout>


    </androidx.appcompat.widget.Toolbar>


    <!--   //TODO   #1 -->


    <com.google.android.material.button.MaterialButton
        android:id="@+id/КнопкаоСистеме"
        style="@style/Widget.MaterialComponents.Button.TextButton.Icon"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/КнопкаПользователи"


        android:layout_centerInParent="true"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:backgroundTint="#FFFFFF"
        android:drawableLeft="@drawable/icon_newsettings_info_settrtings"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:minHeight="70dp"
        android:paddingLeft="15dp"
        android:text="О системе"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="normal"
        android:translationZ="2dp"
        android:visibility="visible"
        app:cardElevation="2dp"
        app:cardMaxElevation="1dp"
        app:cornerRadius="15dp"
        app:iconGravity="textStart"
        app:rippleColor="@color/design_default_color_on_secondary"
        app:strokeColor="#EEEDEF"
        app:strokeWidth="1dp" />


    <!--   //TODO   #2 -->


    <com.google.android.material.button.MaterialButton
        android:id="@+id/КнопкаПользователи"
        style="@style/Widget.MaterialComponents.Button.TextButton.Icon"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/КнопкаОбменДанными"


        android:layout_centerInParent="true"

        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:backgroundTint="#FFFFFF"
        android:drawableLeft="@drawable/icon_newsetting_users_ettings"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:minHeight="70dp"
        android:paddingLeft="15dp"
        android:text="Пользователи"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="normal"
        android:translationZ="2dp"
        android:visibility="visible"
        app:cardElevation="2dp"
        app:cardMaxElevation="1dp"
        app:cornerRadius="15dp"
        app:iconGravity="textStart"
        app:rippleColor="@color/design_default_color_on_secondary"
        app:strokeColor="#EEEDEF"
        app:strokeWidth="1dp" />


    <!--   //TODO   #3 -->


    <com.google.android.material.button.MaterialButton
        android:id="@+id/КнопкаОбменДанными"
        style="@style/Widget.MaterialComponents.Button.TextButton.Icon"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/Кнопкаобновление"


        android:layout_centerInParent="true"

        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:backgroundTint="#FFFFFF"
        android:drawableLeft="@drawable/icon_newsettings_async"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:minHeight="70dp"
        android:paddingLeft="15dp"
        android:text="Обмен данными"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="normal"
        android:translationZ="2dp"
        android:visibility="visible"
        app:cardElevation="2dp"
        app:cardMaxElevation="1dp"
        app:cornerRadius="15dp"
        app:iconGravity="textStart"
        app:rippleColor="@color/design_default_color_on_secondary"
        app:strokeColor="#EEEDEF"
        app:strokeWidth="1dp" />








    <!--   //TODO   #4-->
    <com.google.android.material.button.MaterialButton
        android:id="@+id/Кнопкаобновление"
        style="@style/Widget.MaterialComponents.Button.TextButton.Icon"

        android:layout_above="@+id/КнопкаОшибки"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"



        android:layout_centerInParent="true"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:backgroundTint="#FFFFFF"
        android:drawableLeft="@drawable/icon_newsettings_updatepo"


        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:minHeight="70dp"
        android:paddingLeft="15dp"
        android:text="Обновление"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="normal"
        android:translationZ="2dp"
        android:visibility="visible"
        app:cardElevation="2dp"
        app:cardMaxElevation="1dp"
        app:cornerRadius="15dp"
        app:iconGravity="textStart"
        app:rippleColor="@color/design_default_color_on_secondary"
        app:strokeColor="#EEEDEF"
        app:strokeWidth="1dp" />






    <!--   //TODO   #5-->
    <com.google.android.material.button.MaterialButton
        android:id="@+id/КнопкаОшибки"
        style="@style/Widget.MaterialComponents.Button.TextButton.Icon"

        android:layout_above="@+id/КнопкаШаблоны"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"




        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"

        android:backgroundTint="#FFFFFF"
        android:drawableLeft="@drawable/icon_newsettings_error"


        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:minHeight="70dp"
        android:paddingLeft="15dp"
        android:text="Ошибки"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="normal"
        android:translationZ="2dp"
        android:visibility="visible"
        app:cardElevation="2dp"
        app:cardMaxElevation="1dp"
        app:cornerRadius="15dp"
        app:iconGravity="textStart"
        app:rippleColor="@color/design_default_color_on_secondary"
        app:strokeColor="#EEEDEF"
        app:strokeWidth="1dp" />








    <!--   //TODO   #6-->
    <com.google.android.material.button.MaterialButton
        android:id="@+id/КнопкаШаблоны"
        style="@style/Widget.MaterialComponents.Button.TextButton.Icon"


        android:layout_alignParentBottom="true"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"




        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="30dp"
        android:backgroundTint="#FFFFFF"
        android:drawableLeft="@drawable/icon_newswttings_shablon"


        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:minHeight="70dp"
        android:paddingLeft="15dp"
        android:text="Шаблоны"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="normal"
        android:translationZ="2dp"
        android:visibility="visible"
        app:cardElevation="2dp"
        app:cardMaxElevation="1dp"
        app:cornerRadius="15dp"
        app:iconGravity="textStart"
        app:rippleColor="@color/design_default_color_on_secondary"
        app:strokeColor="#EEEDEF"
        app:strokeWidth="1dp" />














    <!--      //TODO конец всех данных-->

</RelativeLayout>










