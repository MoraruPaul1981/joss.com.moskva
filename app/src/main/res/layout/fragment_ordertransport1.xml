<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/linearLayoutfragmentorderone"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
    android:orientation="vertical">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/TextViewHadler"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginTop="5dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:gravity="center"
            android:hint="  Фрагмент 1 Заказ"
            android:minWidth="100dp"
            android:minHeight="35dp"
            android:padding="2dp"
            android:textAlignment="center"
            android:textColor="#00574B"
            android:textSize="14sp"
            android:textStyle="bold"
            android:visibility="visible"
            tools:ignore="MissingConstraints,RtlCompat" />


        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/view2Линия"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="3dp"
            android:layout_marginTop="2dp"
            android:padding="2dp"
            android:background="#00ACC1"
            android:foregroundGravity="center"
            android:visibility="visible"
            android:layout_centerHorizontal="true" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="horizontal">

        <ImageView
            android:id="@android:id/text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minHeight="45dp"
            android:minWidth="30dp"
            android:padding="2dp"
            android:layout_margin="5dp"
            android:animateLayoutChanges="true"
            android:background="@drawable/stylesingletablerowiine"
            android:gravity="center"
            android:visibility="visible"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:srcCompat="@drawable/icon_alltabels4"
            app:strokeColor="#F5FFFA" />



        <com.google.android.material.textview.MaterialTextView
            android:id="@android:id/text2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="45dp"
            android:padding="2dp"
            android:layout_margin="5dp"
            android:background="@drawable/stylesingletablerowiine"
            android:checkMark="?android:attr/textCheckMark"
            android:foregroundGravity="center"
            android:gravity="center"
            android:paddingStart="?android:attr/listPreferredItemPaddingStart"
            android:paddingEnd="?android:attr/listPreferredItemPaddingEnd"
            android:text="text2"
            android:textAlignment="gravity"
            android:textAppearance="?android:attr/textAppearanceListItemSmall"
            android:textColor="#000000"
            android:textSize="14sp"
            android:textStyle="bold"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:strokeColor="#F5FFFA"
            android:animateLayoutChanges="true"
            android:visibility="visible"/>






    </LinearLayout>








</LinearLayout>
