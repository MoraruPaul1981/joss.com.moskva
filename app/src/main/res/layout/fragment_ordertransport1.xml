<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    android:id="@android:id/text1"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:minHeight="150dp"
    android:layout_margin="10dp"
    android:padding="2dp"
    android:gravity="center"
    tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
    android:orientation="vertical">









<com.google.android.material.card.MaterialCardView
    android:id="@+id/cardveiwnewordertraport"
    android:layout_width="340dp"
    android:layout_height="wrap_content"
    app:layout_gravity="center"
    android:minHeight="150dp"
    app:cardCornerRadius="5dp"
    app:cardElevation="1dp"
    app:cardMaxElevation="10dp"
    app:strokeWidth="1dp"
    android:translationZ="100dp"
    android:animateLayoutChanges="true"
    android:background="#F0FFFF"
    app:cardBackgroundColor="#F0FFFF"
    app:strokeColor="#00838F"
    tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports">


    <LinearLayout
        android:id="@+id/linearLayoutorderstransport"
        android:layout_width="340dp"
        android:layout_height="wrap_content"
        android:background="#215050"
        android:minHeight="150dp"
        android:gravity="center"
        android:padding="1dp"
        android:layout_margin="1dp"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="vertical">

    <ImageView
        android:id="@+id/imageview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        android:animateLayoutChanges="true"
        android:gravity="center"
        android:visibility="visible"
        app:cardCornerRadius="20dp"
        app:cardElevation="10dp"
        app:srcCompat="@drawable/icon_rdertransport1"
        app:strokeColor="#F5FFFA" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/texviewordertransport2"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="3dp"
        android:gravity="center"
        android:padding="2dp"
        android:background="@drawable/divider_for_single_ordertransport"
        android:foregroundGravity="center"
        android:visibility="visible"
        app:cardBackgroundColor="#F0FFFF"
        app:strokeColor="#D2D8D8"
        android:layout_centerHorizontal="true" />




    <!--  НАЧАЛО ПЕРВОГО БЛОКА -->

    <!--  КОНЕЦ ПЕРВОГО БЛОКА -->

    <!--  НАЧАЛО ВТОРОЙ БЛОКА -->
    <LinearLayout
        android:id="@+id/linearLayoutordertransfport1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:gravity="center"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="vertical">
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otkey1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:layout_marginBottom="2dp"
            android:minHeight="10dp"
            android:hint="Цфо"
            android:padding="1dp"
            android:animateLayoutChanges="true"
            android:background="@drawable/stylesingletablerowiine3"
            android:gravity="center"
            android:visibility="visible"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:srcCompat="@drawable/icon_ordertransportorder1"
            app:strokeColor="#F5FFFA" />
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otvaluecfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="15dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="3dp"
            android:padding="1dp"
            android:background="@drawable/stylesingletablerowiine"
            android:checkMark="?android:attr/textCheckMark"
            android:foregroundGravity="center"
            android:gravity="center"
            android:paddingStart="?android:attr/listPreferredItemPaddingStart"
            android:paddingEnd="?android:attr/listPreferredItemPaddingEnd"
            android:hint="Значение ЦФО #2"
            android:textAlignment="gravity"
            android:textAppearance="?android:attr/textAppearanceListItemSmall"
            android:textColor="#000000"
            android:textSize="13sp"
            android:textStyle="bold"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:strokeColor="#F5FFFA"
            android:animateLayoutChanges="true"
            android:visibility="visible"/>
    </LinearLayout>
    <!--  КОНЕЦ ВТОРОЙ БЛОКА -->


    <!--  НАЧАЛО ТРЕТИЙ  БЛОКА -->
    <LinearLayout
        android:id="@+id/linearLayoutordertransfport2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:gravity="center"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="vertical">
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otkey2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:layout_marginBottom="2dp"
            android:minHeight="10dp"
            android:hint="Вид ТС"
            android:padding="1dp"
            android:animateLayoutChanges="true"
            android:background="@drawable/stylesingletablerowiine3"
            android:gravity="center"
            android:visibility="visible"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:srcCompat="@drawable/icon_ordertransportorder1"
            app:strokeColor="#F5FFFA" />
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otvaluetypets"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="15dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="3dp"
            android:padding="1dp"
            android:background="@drawable/stylesingletablerowiine"
            android:checkMark="?android:attr/textCheckMark"
            android:foregroundGravity="center"
            android:gravity="center"
            android:paddingStart="?android:attr/listPreferredItemPaddingStart"
            android:paddingEnd="?android:attr/listPreferredItemPaddingEnd"
            android:hint="Значение Вида ТС #3"
            android:textAlignment="gravity"
            android:textAppearance="?android:attr/textAppearanceListItemSmall"
            android:textColor="#000000"
            android:textSize="14sp"
            android:textStyle="bold"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:strokeColor="#F5FFFA"
            android:animateLayoutChanges="true"
            android:visibility="visible"/>
    </LinearLayout>
    <!--  КОНЕЦ ТРЕТИЙ БЛОКА -->




    <!--  НАЧАЛО ЧЕТВЕРТОЙ  БЛОКА -->
    <LinearLayout
        android:id="@+id/linearLayoutordertransfport3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:gravity="center"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="vertical">
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otkey3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:layout_marginBottom="2dp"
            android:minHeight="15dp"
            android:padding="1dp"
            android:hint="Гос.номер"
            android:animateLayoutChanges="true"
            android:background="@drawable/stylesingletablerowiine3"
            android:gravity="center"
            android:visibility="visible"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:srcCompat="@drawable/icon_ordertransportorder1"
            app:strokeColor="#F5FFFA" />
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otvaluegosnomer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="25dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="3dp"
            android:padding="2dp"
            android:background="@drawable/stylesingletablerowiine"
            android:checkMark="?android:attr/textCheckMark"
            android:foregroundGravity="center"
            android:gravity="center"
            android:paddingStart="?android:attr/listPreferredItemPaddingStart"
            android:paddingEnd="?android:attr/listPreferredItemPaddingEnd"
            android:hint="Значение Гос.номер #4"
            android:textAlignment="gravity"
            android:textAppearance="?android:attr/textAppearanceListItemSmall"
            android:textColor="#000000"
            android:textSize="14sp"
            android:textStyle="bold"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:strokeColor="#F5FFFA"
            android:animateLayoutChanges="true"
            android:visibility="visible"/>
    </LinearLayout>
    <!--  КОНЕЦ ЧЕТВЕРТОЙ БЛОКА -->

    <!--  НАЧАЛО ПЯТОГО  БЛОКА -->
    <LinearLayout
        android:id="@+id/linearLayoutordertransfport4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:gravity="center"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="vertical">
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otkey4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:layout_marginBottom="2dp"
            android:minHeight="10dp"
            android:padding="1dp"
            android:hint="Дата"
            android:animateLayoutChanges="true"
            android:background="@drawable/stylesingletablerowiine3"
            android:gravity="center"
            android:visibility="visible"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:srcCompat="@drawable/icon_ordertransportorder1"
            app:strokeColor="#F5FFFA" />
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otvaluedatesordert"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="15dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="5dp"
            android:padding="1dp"
            android:background="@drawable/stylesingletablerowiine"
            android:checkMark="?android:attr/textCheckMark"
            android:foregroundGravity="center"
            android:gravity="center"
            android:paddingStart="?android:attr/listPreferredItemPaddingStart"
            android:paddingEnd="?android:attr/listPreferredItemPaddingEnd"
            android:hint="Значение Дата #5"
            android:textAlignment="gravity"
            android:textAppearance="?android:attr/textAppearanceListItemSmall"
            android:textColor="#000000"
            android:textSize="14sp"
            android:textStyle="bold"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:strokeColor="#F5FFFA"
            android:animateLayoutChanges="true"
            android:visibility="visible"/>
    </LinearLayout>
    <!--  КОНЕЦ ПЯТОГО БЛОКА -->



    <!--  НАЧАЛО ШЕСТОЙ  БЛОКА -->
    <LinearLayout
        android:id="@+id/linearLayoutordertransfport5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:gravity="center"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="vertical"/>
    <!--  КОНЕЦ ПЯТОГО БЛОКА -->




    </LinearLayout>


</com.google.android.material.card.MaterialCardView>



</LinearLayout>



