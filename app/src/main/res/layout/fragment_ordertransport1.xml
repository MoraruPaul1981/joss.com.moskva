<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@android:id/text1"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:minHeight="150dp"
    android:gravity="center"
    android:layout_margin="5dp"
    android:padding="5dp"
    tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/imageview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        android:animateLayoutChanges="true"
        android:gravity="center"
        android:visibility="visible"
        app:cardCornerRadius="20dp"
        app:cardElevation="10dp"
        app:srcCompat="@drawable/icon_rdertransport1"
        app:strokeColor="#F5FFFA" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/texviewordertransport2"
            android:layout_width="match_parent"
            android:layout_height="3dp"
           android:layout_margin="3dp"
            android:gravity="center"
            android:padding="2dp"
            android:background="@drawable/divider_for_single_ordertransport"
            android:foregroundGravity="center"
            android:visibility="visible"
            android:layout_centerHorizontal="true" />




    <!--  НАЧАЛО ПЕРВОГО БЛОКА -->
    <LinearLayout
        android:id="@+id/linearLayoutordertransfport0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:background="#FFFFFF"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="vertical">
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otkey0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:padding="2dp"
            android:minHeight="20dp"
            android:hint="Номер заказа"
            android:animateLayoutChanges="true"
            android:background="@drawable/stylesingletablerowiine3"
            android:gravity="center"
            android:visibility="visible"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:srcCompat="@drawable/icon_ordertransportorder1"
            app:strokeColor="#F5FFFA" />
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otvalue0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="250dp"
            android:layout_margin="2dp"
            android:padding="2dp"
            android:background="@drawable/stylesingletablerowiineordertranport"
            android:checkMark="?android:attr/textCheckMark"
            android:foregroundGravity="center"
            android:gravity="center"
            android:paddingStart="?android:attr/listPreferredItemPaddingStart"
            android:paddingEnd="?android:attr/listPreferredItemPaddingEnd"
            android:hint="Номер Заказа  #1"
            android:textAlignment="gravity"
            android:textAppearance="?android:attr/textAppearanceListItemSmall"
            android:textColor="#000000"
            android:textSize="14sp"
            android:textStyle="bold"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:strokeColor="#F5FFFA"
            android:animateLayoutChanges="true"
            android:visibility="visible"/>
    </LinearLayout>
    <!--  КОНЕЦ ПЕРВОГО БЛОКА -->

    <!--  НАЧАЛО ВТОРОЙ БЛОКА -->
    <LinearLayout
        android:id="@+id/linearLayoutordertransfport1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:gravity="center"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="vertical">
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otkey1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:minHeight="20dp"
            android:hint="ЦФО"
            android:padding="2dp"
            android:animateLayoutChanges="true"
            android:background="@drawable/stylesingletablerowiine3"
            android:gravity="center"
            android:visibility="visible"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:srcCompat="@drawable/icon_ordertransportorder1"
            app:strokeColor="#F5FFFA" />
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otvalue1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="25dp"
            android:layout_margin="2dp"
            android:padding="2dp"
            android:background="@drawable/stylesingletablerowiine"
            android:checkMark="?android:attr/textCheckMark"
            android:foregroundGravity="center"
            android:gravity="center"
            android:paddingStart="?android:attr/listPreferredItemPaddingStart"
            android:paddingEnd="?android:attr/listPreferredItemPaddingEnd"
            android:hint="Значение ЦФО #2"
            android:textAlignment="gravity"
            android:textAppearance="?android:attr/textAppearanceListItemSmall"
            android:textColor="#000000"
            android:textSize="14sp"
            android:textStyle="bold"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:strokeColor="#F5FFFA"
            android:animateLayoutChanges="true"
            android:visibility="visible"/>
    </LinearLayout>
    <!--  КОНЕЦ ВТОРОЙ БЛОКА -->


    <!--  НАЧАЛО ТРЕТИЙ  БЛОКА -->
    <LinearLayout
        android:id="@+id/linearLayoutordertransfport2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:gravity="center"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="vertical">
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otkey2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:minHeight="20dp"
            android:hint="Вид ТС"
            android:padding="2dp"
            android:animateLayoutChanges="true"
            android:background="@drawable/stylesingletablerowiine3"
            android:gravity="center"
            android:visibility="visible"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:srcCompat="@drawable/icon_ordertransportorder1"
            app:strokeColor="#F5FFFA" />
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otvalue2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="25dp"
            android:layout_margin="2dp"
            android:padding="2dp"
            android:background="@drawable/stylesingletablerowiine"
            android:checkMark="?android:attr/textCheckMark"
            android:foregroundGravity="center"
            android:gravity="center"
            android:paddingStart="?android:attr/listPreferredItemPaddingStart"
            android:paddingEnd="?android:attr/listPreferredItemPaddingEnd"
            android:hint="Значение Вида ТС #3"
            android:textAlignment="gravity"
            android:textAppearance="?android:attr/textAppearanceListItemSmall"
            android:textColor="#000000"
            android:textSize="14sp"
            android:textStyle="bold"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:strokeColor="#F5FFFA"
            android:animateLayoutChanges="true"
            android:visibility="visible"/>
    </LinearLayout>
    <!--  КОНЕЦ ТРЕТИЙ БЛОКА -->




    <!--  НАЧАЛО ЧЕТВЕРТОЙ  БЛОКА -->
    <LinearLayout
        android:id="@+id/linearLayoutordertransfport3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:gravity="center"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="vertical">
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otkey3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:minHeight="20dp"
            android:hint="заказ4"
            android:animateLayoutChanges="true"
            android:background="@drawable/stylesingletablerowiine3"
            android:gravity="center"
            android:visibility="visible"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:srcCompat="@drawable/icon_ordertransportorder1"
            app:strokeColor="#F5FFFA" />
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/otvalue3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="25dp"
            android:layout_margin="2dp"
            android:padding="2dp"
            android:background="@drawable/stylesingletablerowiine"
            android:checkMark="?android:attr/textCheckMark"
            android:foregroundGravity="center"
            android:gravity="center"
            android:paddingStart="?android:attr/listPreferredItemPaddingStart"
            android:paddingEnd="?android:attr/listPreferredItemPaddingEnd"
            android:hint="Заказ #4"
            android:textAlignment="gravity"
            android:textAppearance="?android:attr/textAppearanceListItemSmall"
            android:textColor="#000000"
            android:textSize="14sp"
            android:textStyle="bold"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:strokeColor="#F5FFFA"
            android:animateLayoutChanges="true"
            android:visibility="visible"/>
    </LinearLayout>
    <!--  КОНЕЦ ЧЕТВЕРТОЙ БЛОКА -->



</LinearLayout>
