<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@android:id/text1"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:layout_margin="10dp"
    android:padding="10dp"
    tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
    android:orientation="vertical">




    <ImageView
        android:id="@+id/imageview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minWidth="40dp"
        android:minHeight="25dp"
        android:layout_centerInParent="true"
        android:padding="2dp"
        android:layout_margin="5dp"
        android:animateLayoutChanges="true"
        android:gravity="center"
        android:visibility="visible"
        app:cardCornerRadius="20dp"
        app:cardElevation="10dp"
        app:srcCompat="@drawable/icon_rdertransport1"
        app:strokeColor="#F5FFFA" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/texviewordertransport2"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginStart="3dp"
            android:layout_marginEnd="3dp"
            android:layout_marginBottom="3dp"
            android:layout_marginTop="2dp"
            android:padding="2dp"
            android:background="#797F80"
            android:foregroundGravity="center"
            android:visibility="visible"
            android:layout_centerHorizontal="true" />



    <!--  Первая СТрочка-->

    <LinearLayout
        android:id="@+id/linearLayoutordertransfport0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        tools:context=".CodeOrdersAnTransports.Window.MainActivityOrdersTransports"
        android:orientation="vertical">



        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/key0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="20dp"
            android:hint="Заказ"
            android:layout_margin="2dp"
            android:animateLayoutChanges="true"
            android:background="@drawable/stylesingletablerowiine3"
            android:gravity="center"
            android:visibility="visible"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:srcCompat="@drawable/icon_ordertransportorder1"
            app:strokeColor="#F5FFFA" />



        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/value0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="25dp"
            android:layout_margin="2dp"
            android:background="@drawable/stylesingletablerowiine"
            android:checkMark="?android:attr/textCheckMark"
            android:foregroundGravity="center"
            android:gravity="center"
            android:paddingStart="?android:attr/listPreferredItemPaddingStart"
            android:paddingEnd="?android:attr/listPreferredItemPaddingEnd"
            android:hint="Заказ #1"
            android:textAlignment="gravity"
            android:textAppearance="?android:attr/textAppearanceListItemSmall"
            android:textColor="#000000"
            android:textSize="14sp"
            android:textStyle="bold"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp"
            app:strokeColor="#F5FFFA"
            android:animateLayoutChanges="true"
            android:visibility="visible"/>
    </LinearLayout>

    <!--  КОНЕЦ ПЕРВОГО БЛОКА -->




</LinearLayout>
