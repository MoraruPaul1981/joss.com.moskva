<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@android:id/text1"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:gravity="center"
    tools:context=".OrdersTransports.Window.MainActivityOrdersTransports"
    android:orientation="vertical">





<com.google.android.material.card.MaterialCardView
    android:id="@+id/cardviewnewnewordermateril2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_gravity="center"
    android:minHeight="150dp"
    app:strokeWidth="1dp"
    android:layout_margin="10dp"
    android:padding="2dp"
    android:translationZ="100dp"
    app:cardElevation="1dp"
    app:cardMaxElevation="10dp"
    app:cardCornerRadius="5dp"
    android:animateLayoutChanges="true"
    app:cardBackgroundColor="#F0FFFF"
    app:strokeColor="#D2D8D8"
    tools:context=".OrdersTransports.Window.MainActivityOrdersTransports">




    <!--  НАЧАЛО ПЕРВОГО БЛОКА -->
    <RelativeLayout
        android:id="@+id/linearLayoutordertransfport0"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F0FFFF"
        android:gravity="center"
        android:layout_margin="1dp"
        tools:context=".OrdersTransports.Window.MainActivityOrdersTransports">
        <!--todo новый код созданиеновой задачи-->


        <!--   todo сами элементы начало -->
        <!-- todo#1-->
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/textviewnamecfo"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_centerInParent="true"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="120dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="120dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/style_for_assitiionmatrial_create_name"
            android:gravity="center_vertical|center_horizontal"
            android:hint="Цфо"
            android:textColor="#000000"
            android:textColorHint="#000000"
            android:textSize="13sp"
            android:textStyle="bold"
            android:translationZ="1dp"
            android:visibility="visible"
            app:cardCornerRadius="1dp"
            android:layout_above="@+id/valuecfo"
            app:cardElevation="1dp" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/valuecfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:background="@drawable/style_forpolymaterialb_cycle_createspinner"
            android:gravity="center"
            android:hint="ц"
          android:minHeight="40dp"
            android:textAlignment="gravity"
            android:textColor="#000000"
            android:textColorHighlight="#80807C"
            android:textColorHint="#80807C"
            android:textSize="15sp"
            android:textStyle="bold"
            android:padding="2dp"
            android:layout_above="@+id/textviewnamematerial"
            android:visibility="visible" />


        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/textviewnamematerial"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginStart="120dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="120dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/style_for_assitiionmatrial_create_name"
            android:gravity="center_vertical|center_horizontal"
            android:hint="Вид ТС"
            android:minWidth="50dp"
            android:textColorHint="#000000"
            android:textSize="13sp"
            android:textStyle="bold"
            android:translationZ="1dp"
            android:visibility="visible"
            app:cardCornerRadius="5dp"
            android:layout_above="@+id/valuetypetc"
            app:cardElevation="1dp" />


        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/valuetypetc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:background="@drawable/style_forpolymaterialb_cycle_createspinner"
            android:gravity="center"
            android:hint="вид"
            android:padding="2dp"
          android:minHeight="40dp"
            android:textAlignment="gravity"
            android:textColor="#000000"
            android:textColorHighlight="#80807C"
            android:textColorHint="#80807C"
            android:textSize="15sp"
            android:textStyle="bold"
            android:layout_above="@+id/textviewnamematerials"
            android:visibility="visible" />


        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/textviewnamematerials"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginStart="120dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="120dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/style_for_assitiionmatrial_create_name"
            android:gravity="center_vertical|center_horizontal"
            android:hint="Гос.номер"
            android:minWidth="50dp"
            android:textColor="#000000"
            android:textColorHint="#000000"
            android:textSize="13sp"
            android:textStyle="bold"
            android:translationZ="1dp"
            android:visibility="gone"
            app:cardCornerRadius="5dp"
            android:layout_above="@+id/valuegosnomer"
            app:cardElevation="1dp" />


        <!-- todo старрт добавление еще двух полей стак и компания-->

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/valuegosnomer"
            style="@android:style/Widget.Holo.TextView.SpinnerItem"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="7dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="3dp"
            android:layout_marginBottom="-3dp"
            android:background="@drawable/style_forpolymaterialb_cycle_createspinner"
            android:gravity="center"
            android:hint="гос"
          android:minHeight="40dp"
            android:textAlignment="gravity"
            android:textColor="#000000"
            android:textColorHighlight="#80807C"
            android:textColorHint="#80807C"
            android:textSize="15sp"
            android:textStyle="bold"
            android:layout_above="@+id/textviewtcomnapy"
            android:visibility="gone" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/textviewtcomnapy"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginStart="120dp"
            android:layout_above="@+id/valuedate"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="120dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/style_for_assitiionmatrial_create_name"
            android:gravity="center_vertical|center_horizontal"
            android:hint="Дата"
            android:minWidth="50dp"
            android:textColor="#000000"
            android:textColorHint="#000000"
            android:textSize="13sp"
            android:textStyle="bold"
            android:translationZ="1dp"
            android:visibility="visible"
            app:cardCornerRadius="5dp"
            app:cardElevation="1dp" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/valuedate"
            style="@android:style/Widget.Holo.TextView.SpinnerItem"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:layout_above="@+id/bottomnewordertransport"
            android:background="@drawable/style_forpolymaterialb_cycle_createspinner"
            android:gravity="center"
            android:hint="дата"
            android:padding="2dp"
          android:minHeight="40dp"
            android:textAlignment="gravity"
            android:textColor="#000000"
            android:textColorHighlight="#80807C"
            android:textColorHint="#80807C"
            android:textSize="15sp"
            android:textStyle="bold"
            android:visibility="visible" />



        <!--  todo кнопка для сохранения-->
        <com.google.android.material.button.MaterialButton
            android:id="@+id/bottomnewordertransport"
            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton.Icon"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerInParent="true"
            android:layout_marginTop="100dp"
            android:clickable="true"
            android:enabled="true"
            android:focusable="auto"
            android:fontFamily="sans-serif-medium"
            android:foregroundGravity="center"
            android:gravity="center"
            android:minHeight="35dp"
            android:text="Сохранить"
            android:textColor="#0E0E0E"
            android:textSize="9sp"
            android:textStyle="bold"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:translationZ="150dp"
            android:visibility="visible"
            app:backgroundTint="#00E5FF"
            app:cardElevation="1dp"
            app:cardMaxElevation="10dp"
            app:cornerRadius="20dp"
            tools:visibility="visible" />


    </RelativeLayout>
    <!--  КОНЕЦ ПЯТОГО БЛОКА -->
</com.google.android.material.card.MaterialCardView>


</LinearLayout>






