    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    android:padding="2dp"
    app:cardCornerRadius="10dp"
    app:cardElevation="10dp"
    app:strokeColor="#ABB1B1"
    app:cardBackgroundColor="#F1F4F4"
    app:cardMaxElevation="30dp"
    app:strokeWidth="1dp"
    android:translationZ="100dp"
    android:animateLayoutChanges="true"
    android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
    tools:context=".CommitingPrices.View.Window.MainActivityCommitingPrices">




    <TableLayout
        android:shrinkColumns="*"
        android:stretchColumns="*"
        android:gravity="center_horizontal|center_vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:padding="1dp"
            android:background="@drawable/disayn_line_tablerow10"
            android:gravity="center_vertical|center_horizontal">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <!--TODO второая страка -->
                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/mTV_NumberRow_value"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_column="0"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_centerInParent="true"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:background="@drawable/style_forpolymaterialb_cycle_createspinner2"
                    android:gravity="center"
                    android:minHeight="25dp"
                    android:text="№"
                    android:textAlignment="gravity"
                    android:textColor="#000000"
                    android:textColorHint="#80807C"
                    android:textSize="14sp"
                    android:textStyle="normal"
                    android:visibility="visible"
                    app:fontFamily="@font/average_sans" />


                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/mTV_commitingprices_count"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_column="1"
                    android:layout_alignParentTop="true"
                    android:layout_centerInParent="true"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="3dp"
                    android:background="@drawable/style_forpolymaterialb_cycle_createspinner15"
                    android:gravity="center"
                    android:hint="сумма сог.#"
                    android:minWidth="100dp"
                    android:textAlignment="gravity"
                    android:textColor="#000000"
                    android:textColorHint="#454542"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:visibility="visible"
                    app:fontFamily="@font/average_sans" />


                <com.google.android.material.button.MaterialButton
                    android:id="@+id/arrow_nested_receriview"
                    android:layout_width="36dp"
                    android:layout_height="wrap_content"
                    android:layout_column="2"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginEnd="5dp"
                    android:backgroundTint="#F1F4F4"
                    android:fontFamily="@font/alegreya_sans_sc_thin"
                    android:gravity="center"
                    android:padding="2dp"
                    android:stateListAnimator="@drawable/icon_for_commintprices_successs"
                    android:textAlignment="center"
                    app:cornerRadius="20dp"
                    app:icon="@drawable/icon_for_commintprices_successs"
                    app:iconGravity="textEnd"
                    app:iconPadding="1dp"
                    app:iconSize="20dp"
                    app:iconTint="#909393"
                    app:rippleColor="#F50057"
                    app:strokeColor="#ABB1B1"
                    app:strokeWidth="1dp"
                    tools:ignore="SpeakableTextPresentCheck" />
            </RelativeLayout>
        </TableRow>




